<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.awesome.amuclient">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE " />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Amuclient"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.main.view.ReviewActivity"></activity>
        <activity android:name=".ui.main.view.PromotionActivity" />
        <activity android:name=".ui.main.view.ReserveListActivity" />
        <activity android:name=".ui.main.view.ReserveDetailActivity" />
        <activity android:name=".ui.main.view.ReserveActivity" />
        <activity android:name=".ui.main.view.StoreInfoActivity" />
        <activity android:name=".ui.main.view.JoinInfoActivity" />
        <activity android:name=".ui.main.view.JoinActivity" />
        <activity android:name=".ui.main.view.MyPageActivity" />
        <activity android:name=".ui.main.view.LoginActivity" />
        <activity android:name=".ui.main.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="bf76172ec190e0807ea9c7fcde481749" />
    </application>

</manifest>